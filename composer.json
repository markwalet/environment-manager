{
    "name": "markwalet/dotenv-manager",
    "description": "A Laravel package that helps you edit the .env file programmatically.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Walet",
            "email": "mark.walet@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "MarkWalet\\EnvironmentManager\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarkWalet\\EnvironmentManager\\EnvironmentManagerServiceProvider"
            ]
        }
    }
}
